{
  "name": "open-stage-control-builder",
  "description": "[Builder package] A libre desktop OSC bi-directionnal control surface application",
  "version": "0.21.0",
  "author": {
    "name": "Jean-Emmanuel Doucet / AMMD",
    "email": "jeannot@ammd.net"
  },
  "keywords": [
    "osc",
    "midi",
    "interface",
    "multi-touch"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jean-emmanuel/open-stage-control"
  },
  "homepage": "http://osc.ammd.net/",
  "license": "GPL-3.0",
  "devDependencies": {
    "babelify": "7.3.0",
    "browserify": "14.4.0",
    "exorcist": "1.0.0",
    "licensify": "3.1.3",
  },
  "_dependencies": {
    "autolayout": "0.7.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "*",
    "cardinal-spline-js": "2.3.6",
    "electron": "~1.8.1",
    "electron-packager": "9.1.0",
    "file-saver": "1.3.3",
    "jquery": "3.2.1",
    "json5": "0.5.1",
    "kiwi.js": "1.0.1",
    "mathjs": "3.16.4",
    "node-sass": "4.5.3",
    "nodemon": "1.12.0",
    "nosleep.js": "0.7.0",
    "screenfull": "3.3.1",
    "shortid": "2.2.8",
    "socket.io-client": "2.0.3",
    "uglifyify": "4.0.4"
  },
  "scripts": {
    "postinstall": "cp resources/images/logo.ico browser/favicon.ico && cd app && npm install && cd .. && npm run build-js && npm run build-css",
    "start": "node index.js",
    "build": "npm run build-js -- --prod && npm run build-css",
    "watch-css": "nodemon --watch src/browser/scss -e scss -x \"npm run build-css && node scripts/reload-css.js\"",
    "watch-js": "nodemon --watch src/browser/js -e js -x \"npm run build-js\"",
    "watch-themes": "nodemon --watch main/themes -e css -x \"node scripts/reload-css.js\"",
    "build-css": "node-sass --include-path src/browser/scss src/browser/scss/style.scss browser/styles.css --output-style compressed",
    "build-js": "node scripts/build-js.js > browser/scripts.js",
    "deploy-docs": "cd resources && mkdocs gh-deploy --clean && rm -rf site",
    "test": "npm run build-js && npm run build-css"
  },
  "engines": {
    "node": "6"
  }
}
