{
  "name": "open-stage-control-web",
  "productName": "Open Stage Control",
  "description": "A libre web OSC bi-directionnal control surface application (fork of the electron version)",
  "open-stage-control-version": "0.21.0",
  "version": "0.0.1",
  "author": {
    "name": "Jean-Emmanuel Doucet / AMMD",
    "email": "jeannot@ammd.net"
  },
  "keywords": [
    "osc",
    "midi",
    "interface"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:amir-arad/open-stage-control.git"
  },
  "homepage": "http://osc.ammd.net/",
  "license": "GPL-3.0",
  "scripts": {
    "start": "node index.js",
    "debug": "node --inspect-brk index.js",
    "build": "npm run build-js -- --prod && npm run build-css",
    "watch-css": "nodemon --watch src/browser/scss -e scss -x \"npm run build-css && node scripts/reload-css.js\"",
    "watch-js": "nodemon --watch src/browser/js -e js -x \"npm run build-js\"",
    "watch-themes": "nodemon --watch main/themes -e css -x \"node scripts/reload-css.js\"",
    "build-css": "node-sass --include-path src/browser/scss src/browser/scss/style.scss browser/styles.css --output-style compressed",
    "build-js": "node scripts/build-js.js > browser/scripts.js",
    "deploy-docs": "cd resources && mkdocs gh-deploy --clean && rm -rf site",
    "test": "npm run build-js && npm run build-css"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "*",
    "autolayout": "0.7.0",
    "cardinal-spline-js": "2.3.6",
    "babelify": "7.3.0",
    "browserify": "14.4.0",
    "exorcist": "1.0.0",
    "file-saver": "1.3.3",
    "json5": "0.5.1",
    "kiwi.js": "1.0.1",
    "licensify": "3.1.3",
    "mathjs": "3.16.4",
    "node-sass": "4.5.3",
    "nodemon": "1.12.0",
    "nosleep.js": "0.7.0",
    "screenfull": "3.3.1",
    "shortid": "2.2.8",
    "socket.io-client": "2.0.3",
    "uglifyify": "4.0.4"
  },
  "dependencies": {
    "vmodule":"1.0.1",
    "osc": "2.2.0",
    "yargs": "9.0.1",
    "socket.io": "2.0.3",
    "express": "4.16.1",
    "jquery": "3.2.1",
    "chokidar": "1.7.0",
    "python-shell":"0.4.0"
  },
  "engines": {
    "node": "6"
  }
}
